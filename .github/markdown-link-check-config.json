    400,
    401,
    403,
    404,
    408,
    429,
    500,
    502,
    503,
    504
  ],
  "httpHeaders": [
    {
      "headers": {
        "Accept": "text/html"
      },
      "urls": [
        "https://github.com"
      ]
    }
  ],
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://localhost"
    },
    {
      "pattern": "^http://127.0.0.1"
    },
    {
      "pattern": "^https://127.0.0.1"
    }
  ],
  "retryCount": 3,
  "retryOn429": true,
  "timeout": "20s"
}
